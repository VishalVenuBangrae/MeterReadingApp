<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.internshala.smartappformeterreading">

    <application
        android:allowBackup="true"
        android:icon="@drawable/meter_logo_new"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SmartAppForMeterReading">
        <activity android:name=".PaymentActivity"></activity>
        <activity android:name=".SplashPayActivity" />
        <activity android:name=".BillActivity" />
        <activity android:name=".ScanElectricityActivity" />
        <activity android:name=".DashBoardActivity" />
        <activity android:name=".RegisterYourselfActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>